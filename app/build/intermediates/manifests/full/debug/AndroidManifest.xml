        </activity>
        <activity android:name="com.imooc.YnuMobile.DBConnect.SQLiteActivity" />
        <activity android:name="com.imooc.YnuMobile.Map.MapmainActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
